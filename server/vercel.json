{
	"version": 2,
   "builds": [
    {
        "src": "./src/index.js",
        "use": "@vercel/node"
    }
   ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ],
   "routes": [
    {
        "src": "/(.*)",
        "dest": "/"
    }
   ]
}