{
    "name": "confeitaria",
    "version": "1.0.0",
    "description": "Website created with Nodejs, React and MySQL to manage a confectionery",
    "main": "index.js",
    "scripts": {
        "start": "node ./src/index.js",
        "dev": "nodemon ./src/index.js",
        "both:dev": "concurrently --kill-others-on-fail \"npm run server:dev\" \"npm run client\"",
        "both:prod": "concurrently --kill-others-on-fail \"npm run server:prod\" \"npm run client\"",
        "server:dev": "nodemon ./src/index.js",
        "server:prod": "pm2 start --no-daemon ./src/index.js --watch --name sweetd-server",
        "client": "cd ../client && npm start",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "nodejs",
        "mysql"
    ],
    "author": "Christhian Rezende Vieira",
    "license": "ISC",
    "engines": {
        "node": "14.x",
        "npm": "6.x"
    },
    "dependencies": {
        "axios": "^0.26.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
        "cloudinary": "^1.28.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^14.2.0",
        "express": "^4.17.2",
        "express-favicon": "^2.0.1",
        "express-fileupload": "^1.2.1",
        "express-session": "^1.17.2",
        "express-validator": "^6.14.0",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mysql2": "^2.3.3",
        "node-schedule": "^2.1.0",
        "nodemailer": "^6.7.2",
        "sequelize": "^6.13.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "nodemon": "^2.0.15"
    }
}
